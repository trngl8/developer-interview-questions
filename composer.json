{
    "name": "trngl8/developer-interview-questions",
    "require": {
        "php": ">=7.4",
        "ext-pdo": "*",
        "symfony/dotenv": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "tests": [
            "php vendor/bin/phpunit tests"
        ]
    }
}
