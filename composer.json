{
    "name": "trngl8/developer-interview-questions",
    "require": {
        "php": ">=7.4",
        "ext-pdo": "*",
        "ext-sqlite3": "*",
        "ext-json": "*",
        "symfony/dotenv": "^6.3",
        "twig/twig": "^3.0",
        "monolog/monolog": "^3.5",
        "symfony/http-client": "^6.3",
        "symfony/http-foundation": "^6.3",
        "symfony/form": "^6.3",
        "symfony/mailer": "^6.3",
        "symfony/twig-bundle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\Unit\\": "tests/unit/",
            "App\\Tests\\E2E\\": "tests/e2e/"
        }
    },
    "scripts": {
        "init-db": [
            "php bin/app"
        ],
        "tests": [
            "php vendor/bin/phpunit tests/unit"
        ]
    }
}
